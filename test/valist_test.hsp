	expected = 0, 1, 3
	repeat 3
		assert valist_get(con, cnt) == expected(cnt)
	loop
	
	//QÆ‰»
	con = valist_make_iota(4)
	valist_dup con, 2, r
	assert r == 2
	r = 99
	assert valist_get(con, 2) == 99
	
	con = valist_make_iota(3)
	valist_ref(con, 1) ++
	assert valist_get(con, 1) == 2
	
	//‘‚«Š·‚¦
	con = valist_make_iota(4)
	valist_set con, str_var, 2
	assert valist_size(con) == 4
	assert valist_get(con, 0) == 0
	assert valist_get(con, 1) == 1
	assert valist_get(con, 2) == str_var
	assert valist_get(con, 3) == 3
	
	//ˆÚ“®AŒğŠ·A„‰ñA”½“]
	
	//Á‹
	con = valist_make_iota(3)
	valist_clear con
	assert valist_size(con) == 0
	
	//˜AŒ‹
	con0 = valist_make_iota(3)
	con1 = valist_make_iota(4)
	valist_chain con0, con1
	assert valist_size(con0) == 3+4
	repeat 3
		assert valist_get(con0, cnt) == cnt
	loop
	repeat 4
		assert valist_get(con0, 3 + cnt) == valist_get(con1, cnt)
	loop
	
	//®—ñ
	
	//’·‚³0
	con0 = valist_make_iota(0)
	valist_sort con0
	assert valist_size(con0) == 0
	
	//—”
	con1 = valist_make(5)
	repeat valist_size(con1)
		valist_set con1, rnd(1000), cnt
	loop
	valist_sort con1
	assert valist_size(con1) == 5
	repeat valist_size(con1) - 1
		assert valist_get(con1, cnt) <= valist_get(con1, cnt + 1)
	loop
	
	//•¡”‚ÌŒ^
	con2 = valist_make()
	valist_push_back con2, "b"
