#include "../abdata/valist.hsp"
#include "../abdata/mod_ref.hsp"

#ifndef true
 #define global true 1
#endif

#module
#deffunc valist_logmes int self
	repeat valist_size(self)
		logmes strf("#%04d::%d  =  %s", cnt, valist_vartype(self, cnt), str(valist_get(self, cnt)))
	loop
	return

#defcfunc valist_make_iota int n,  \
	local self
	
	self = valist_make()
	repeat n
		valist_push_back self, cnt
	loop
	return self
#global

#module
#deffunc valistTest_eq_ran var self, int _value, int sort_mode,  int x_lb, int x_ub, \
	local value, local lb, local ub
	
	value = _value
	valist_equal_range self, value, lb, ub, sort_mode
	assert lb == x_lb
	assert ub == x_ub
	return
	
#deffunc valistTest_ord_insert var _self, int _value, int can_dup, int sort_mode, \
	int x_lb, int x_ub, \
	local self, local value, local lb, local ub
	
	self = valist_make()
	valist_copy self, _self
	value = _value
	valist_ord_insertv self, value, can_dup, sort_mode
	valist_equal_range self, value, lb, ub,  sort_mode
	assert lb == x_lb
	assert ub == x_ub
	return
	
#deffunc valistTest_ord_erase var _self, int _value, int max_count, int sort_mode,  int x_lb, int x_ub, \
	local self, local value, local lb, local ub
	
	self = valist_make()
	valist_copy self, _self
	value = _value
	valist_ord_erasev  self, value, max_count, sort_mode
	valist_equal_range self, value, lb, ub,    sort_mode
	assert lb == x_lb
	assert ub == x_ub
	return
	
#deffunc valistTest_main

	randomize

	//\’zƒeƒXƒg
	
