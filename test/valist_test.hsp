	assert valist_get(con, 2) == M_PI
	repeat 3, 3
		assert valist_get(con, cnt) == cnt - 1
	loop
	
	//insert after last
	con = valist_make_iota(3)
	valist_insert con, 7, 7
	assert valist_size(con) == 8
	repeat 8
		if ( cnt < 3 || cnt == 7 ) {
			assert valist_get(con, cnt) == cnt
		} else {
			assert valist_get(con, cnt) == stt_zero@
		}
	loop
	
	//ƒTƒCƒYw’è
	
	//to extend
	con = valist_make_iota(3)
	valist_resize con, 5, str_var
	assert valist_size(con) == 5
	repeat 5
		if ( cnt < 3 ) {
			assert valist_get(con, cnt) == cnt
		} else {
			assert valist_get(con, cnt) == str_var
		}
