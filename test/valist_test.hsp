	//ŒŸõ
	valistTest_eq_ran con1, 0,,   0, 0
	valistTest_eq_ran con1, 2,,   1, 1
	valistTest_eq_ran con1, 3,,   1, 3
	valistTest_eq_ran con1, 8,,   7, 8
	valistTest_eq_ran con1, 9,,   8, 8
	
	//®—ñÏ‚İ‘}“ü
	//‘}“üŒã‚ÉA‘}“ü‚µ‚½’l‚Ì equal_range ‚ª‚Ç‚¤‚È‚é‚©
	valistTest_ord_insert con1, 0,  true,,    0, 1
	valistTest_ord_insert con1, 1,  true,,    0, 2
	valistTest_ord_insert con1, 1, false,,    0, 1
	valistTest_ord_insert con1, 2,  true,,    1, 2
	valistTest_ord_insert con1, 3,  true,,    1, 4
	valistTest_ord_insert con1, 3, false,,    1, 3
	valistTest_ord_insert con1, 9,  true,,    8, 9
	valistTest_ord_insert con1, 9, false,,    8, 9
	
	//®—ñÏ‚İ‚Ìœ‹
	//œ‹Œã‚ÉAœ‹‚³‚ê‚½’l‚Ì equal_range ‚ª‚Ç‚¤‚È‚é‚©
	valistTest_ord_erase con1, 0,  1,,    0, 0
	valistTest_ord_erase con1, 1,  1,,    0, 0
	valistTest_ord_erase con1, 3,  1,,    1, 2
	valistTest_ord_erase con1, 3, 99,,    1, 1
