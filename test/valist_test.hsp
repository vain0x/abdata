	valist_set con, 99, 2
	assert valist_is_sorted(con, abdata_sort_ascending) == 0
	assert valist_is_sorted(con, abdata_sort_decending) == 0
	
	//‰ºŠEAãŠE
	con1 = valist_make()
	expected = 1, 3, 3, 5, 5, 5, 7, 8
	foreach expected
		valist_pushv_back con1, expected(cnt)
