	con2 = valist_make()
	valist_chain   con2, con1
	valist_reverse con2 //8, 7, 5, 5, 5, 3, 3, 1
	
	valistTest_eq_ran con2, 9, abdata_sort_decending,   0, 0
	valistTest_eq_ran con2, 8, abdata_sort_decending,   0, 1
	valistTest_eq_ran con2, 5, abdata_sort_decending,   2, 5
	valistTest_eq_ran con2, 0, abdata_sort_decending,   8, 8
	
	//®—ñÏ‚İ‘}“ü
	//‘}“üŒã‚ÉA‘}“ü‚µ‚½’l‚Ì equal_range ‚ª‚Ç‚¤‚È‚é‚©
	valistTest_ord_insert con2, 9,  true, abdata_sort_decending,    0, 1
	valistTest_ord_insert con2, 8,  true, abdata_sort_decending,    0, 2
	valistTest_ord_insert con2, 7, false, abdata_sort_decending,    1, 2
	valistTest_ord_insert con2, 5,  true, abdata_sort_decending,    2, 6
	valistTest_ord_insert con2, 0,  true, abdata_sort_decending,    8, 9
	
	//®—ñÏ‚İ‚Ìœ‹
	//œ‹Œã‚ÉAœ‹‚³‚ê‚½’l‚Ì equal_range ‚ª‚Ç‚¤‚È‚é‚©
	valistTest_ord_erase con2, 9,  1, abdata_sort_decending,    0, 0
	valistTest_ord_erase con2, 5,  1, abdata_sort_decending,    2, 4
	valistTest_ord_erase con2, 3, 99, abdata_sort_decending,    5, 5
